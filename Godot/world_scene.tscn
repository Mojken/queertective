[sub_resource type="GDScript" id="GDScript_hkoym"]
script/source = "extends StaticBody3D

var timer : float = 0

func _physics_process(delta):
    timer += delta

    if timer < 3:
        pass
    elif timer < 6:
        position.y = lerp(0, 2, (timer - 3) / 3.0)
    elif timer < 8:
        pass
    elif timer < 11:
        position.y = lerp(2, 0, (timer - 8) / 3.0)
    elif timer >= 11:
        timer = 0
"

