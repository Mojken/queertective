[sub_resource type="GDScript" id="GDScript_l0fj2"]
script/source = "extends Button

@export var action : StringName = \"\"

var replace = {
    \"Up\": \"↑\",
    \"Down\": \"↓\",
    \"Left\": \"←\",
    \"Right\": \"→\",
    \"(Unset)\": \"\",
    \"Escape\": \"Esc\",
    \"Space\": \"␣\",
    \"Exclam\": \"!\",
    \"QuoteDbl\": '\"',
    \"NumberSign\": \"#\",
    \"Dollar\": \"$\",
    \"Percent\": \"%\",
    \"Ampersand\": \"&\",
    \"Apostrophe\": \"'\",
    \"ParenLeft\": \"(\",
    \"ParenRight\": \")\",
    \"Asterisk\": \"*\",
    \"Plus\": \"+\",
    \"Comma\": \",\",
    \"Minus\": \"-\",
    \"Period\": \".\",
    \"Slash\": \"/\",
    \"Colon\": \":\",
    \"Semicolon\": \";\",
    \"Less\": \"<\",
    \"Equal\": \"=\",
    \"Greater\": \">\",
    \"Question\": \"?\",
    \"BracketLeft\": \"[\",
    \"BackSlash\": \"\\\\\",
    \"BracketRight\": \"]\",
    \"AsciiCircum\": \"^\",
    \"UnderScore\": \"_\",
    \"QuoteLeft\": \"`\",
    \"BraceLeft\": \"}\",
    \"Bar\": \"|\",
    \"BraceRight\": \"{\",
    \"AsciiTilde\": \"~\",
}

func _ready():
    self.disabled = false
    var inputs = InputMap.action_get_events(action)
    self.text = \"\"
    for button in inputs:
        button = button as InputEventKey
        var alts = [button.as_text_keycode(), button.as_text_key_label(), OS.get_keycode_string(button.unicode)]
        for alt in alts:
            if alt in replace:
                self.text += replace[alt]
            else:
                self.text += alt.substr(0,3).to_upper()

func _process(_delta):
    if Input.is_action_pressed(action):
        self.button_pressed = true
    else:
        self.button_pressed = false
"

